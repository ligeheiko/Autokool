@page
@using Autokool.Pages.Common.Extensions
@model Autokool.Pages.Autokool.Students.CoursesStudentPage

@{
    Layout = "_Index";
    var page = Model.PageUrl;
    string reg = "";
}

@section Create {
}

@if (Model._registerCourse?.IsRegistered ?? false)
{
    reg = "Cancel registration";
    @Html.ShowTable(Model, null, reg, null, null)
}
else
{
    reg = "Register";
    @Html.ShowTable(Model, null, null, reg, null)
}