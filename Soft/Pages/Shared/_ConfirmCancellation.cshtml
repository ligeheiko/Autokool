@{
    Layout = "_Layout";
    ViewData["Title"] = Model.PageTitle;
    var itemId = Model.ItemId;
}

<p>
    <h1>@ViewData["Title"]</h1>
</p>

<div>
    <h4>Confirm Cancellation</h4>
    <hr />
    <dl class="row">
        @RenderBody()
    </dl>
</div>
<div>
    <a id="button"
       asp-page="./Index"
       asp-route-handler="Index"
       asp-route-sortorder="@Model.SortOrder"
       asp-route-fixedFilter="@Model.FixedFilter"
       asp-route-fixedValue="@Model.FixedValue"
       asp-route-pageIndex="@Model.PageIndex"
       asp-route-IsRegistered="@Model.IsRegistered"
       asp-route-id="@itemId">Confirm</a> |

    <a id="button"
       asp-page="./Index"
       asp-route-handler="Index"
       asp-route-sortorder="@Model.SortOrder"
       asp-route-fixedFilter="@Model.FixedFilter"
       asp-route-fixedValue="@Model.FixedValue"
       asp-route-pageIndex="@Model.PageIndex"
       asp-route-IsRegistered=True
       asp-route-id="@itemId">Cancel</a>
</div>
